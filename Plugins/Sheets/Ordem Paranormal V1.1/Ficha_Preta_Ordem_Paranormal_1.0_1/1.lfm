		<script>
			local function rolarPericia(nomePericia, dados, bonus, treino, outros)
					local soma =0;
					local aux = {};
					local personagem = Firecast.getPersonagemDe(sheet);
					if (personagem ~= nil) then
							local mesa = personagem.mesa;
							if ((personagem.dono == mesa.meuJogador) or (mesa.meuJogador.isMestre)) then

								local dadosR = "ord"..dados.."d20"
								local roll = Firecast.interpretarRolagem(dadosR);
								
								if (bonus~= nil) then
									roll:concatenar(bonus);
									soma = soma + tonumber(bonus);
								end;
								if (treino ~= nil) then
									roll:concatenar(treino);
									soma = soma + tonumber(treino);
								end;
								if (outros ~= nil) then
									roll:concatenar(outros);
									soma = soma + tonumber(outros);
								end;


										local mesaDoPersonagem = Firecast.getMesaDe(sheet);

										if mesaDoPersonagem ~= nil then
												local promise = mesaDoPersonagem.chat:asyncRoll(roll, nomePericia);
												local n, rollObject, logRec = await(promise);
												
												for i = 1, #rollObject.ops, 1 do
													local op = rollObject.ops[i];       
											 
													if op.tipo == "dado" then
															for j = 1, #op.resultados, 1 do
																	aux[#aux + 1] = op.resultados[j];
															end;
													end;
												end;
											
											mesaDoPersonagem.chat:enviarMensagem("[§K10]Resultado: [§K1]".. tonumber(aux[1]) + tonumber(soma) .. " (" .. aux[1] .. " + " .. soma .. ")")	
									   end;
							end;
					end;
			end;
			
			
			local function rolarAtaque(Descricao_Atq, Teste_Atq, Dano_Atq, Margem, Crit, Tipo)
					local soma_atq;
					local aux_atq = {};
					local personagem = Firecast.getPersonagemDe(sheet);
					if (personagem ~= nil) then
							local mesa = personagem.mesa;
							
							if ((personagem.dono == mesa.meuJogador) or (mesa.meuJogador.isMestre)) then
									
									local dadosTeste = "ord".. Teste_Atq;
									local rolagem_teste = Firecast.interpretarRolagem(dadosTeste);
									
									local dadosDano =  "ord".. Dano_Atq;
									local rolagem_dano = Firecast.interpretarRolagem(dadosDano);
									
										if (rolagem_teste.possuiAlgumDado) and (rolagem_dano.possuiAlgumDado) and (Descricao_Atq ~= nil) and (Margem ~= nil) and (Crit ~= nil) then
												local mesaDoPersonagem = Firecast.getMesaDe(sheet);

												if mesaDoPersonagem ~= nil then
														local promise = mesaDoPersonagem.chat:asyncRoll(rolagem_teste, Descricao_Atq);
														local n, rollObject, logRec = await(promise);
														for i = 1, #rollObject.ops, 1 do
															local op = rollObject.ops[i];       
													 
															if op.tipo == "dado" then
																	for j = 1, #op.resultados, 1 do
																			aux_atq[#aux_atq + 1] = op.resultados[j];
																	end;
															elseif op.tipo == "imediato" then
																if op.valor ~= nil then
																	soma_atq = op.valor;
																	
																else
																	soma_atq = 0;
																end;
															end;
														end;
											
														mesaDoPersonagem.chat:enviarMensagem("[§K10]Resultado: [§K1]".. tonumber(aux_atq[1]) + tonumber(soma_atq) .. " (" .. aux_atq[1] .. " + " .. soma_atq .. ")")
														
														local dano = mesaDoPersonagem.chat:asyncRoll(rolagem_dano, string.format("Dano %s", Tipo));
														local m, danoRoll, logRec2 = await(dano);
														
														if (tonumber(aux_atq[1]) >= tonumber(Margem)) then
															mesaDoPersonagem.chat:enviarMensagem("[§K4]ATAQUE CRÍTICO! [§K1](Margem = " .. Margem ..  ")\n[§K4]Dano causado: [§K1]".. m * Crit .. " (" .. m .. " * " .. Crit .. ")");
														end;
												end;
										else
											showMessage("Algum campo do ataque está incompleto, preencha para poder atacar!")
										end; 

										
							end;
					end;
			end;
		</script>
            
